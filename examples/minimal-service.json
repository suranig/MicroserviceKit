{
  "microserviceName": "UserService",
  "namespace": "MyApp.UserService",
  "architecture": {
    "level": "minimal"
  },
  "domain": {
    "aggregates": [
      {
        "name": "User",
        "properties": [
          { "name": "Email", "type": "string" },
          { "name": "Name", "type": "string" }
        ],
        "operations": ["Create", "UpdateEmail"]
      }
    ]
  }
}

